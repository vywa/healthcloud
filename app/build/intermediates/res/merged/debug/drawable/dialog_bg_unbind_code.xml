<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 设置注册界面上获取验证码按钮 -->
    <item>
        <shape>
            <stroke
                android:width="1px"
                android:color="#ccc" /> <!-- 边框颜色 -->
            <solid android:color="#ccc" /> <!-- 填充色 -->
            <corners
                android:bottomRightRadius="10dp"
                android:topRightRadius="10dp" /> <!-- 圆角弧度 -->
        </shape>
    </item>

</layer-list>